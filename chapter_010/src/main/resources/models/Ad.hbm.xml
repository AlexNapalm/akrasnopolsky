<hibernate-mapping>
    <class name="ru.job4j.ioc.models.Ad" table="ads_cs">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <many-to-one name="carbrand" column="carbrand_id" class="ru.job4j.ioc.models.CarBrand" lazy="false"/>
        <property name="model" column="model"/>
        <property name="description" column="description"/>
        <property name="year" column="manufacture_year"/>
        <many-to-one name="cartype" column="cartype_id" class="ru.job4j.ioc.models.CarType" lazy="false"/>
        <property name="price" column="price"/>
        <property name="created" column="publish_date"/>
        <property name="sold" column="sold"/>
        <many-to-one name="user" column="user_id" class="ru.job4j.ioc.models.User" lazy="false"/>
    </class>
</hibernate-mapping>